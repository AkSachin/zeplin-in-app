{
  "manifest_version": 3,
  "name": "Zeplin Open In App",
  "version": "0.1",
  "description": "Opens a zeplin current tab screen url in zeplin app",
  "author": "AkSachin",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
